-- read is missing identifier
program prog12 is
	number: integer;
	procedure double(n: value integer ) is
		n2: integerl
	begin
		n2 := n + n;
		write (int2string(n2));
	end;
begin
	write "Enter a number: ";
        read;
        write "The number doubled is: ";
        double(number);
        writeln ".";
end;

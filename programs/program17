program prog17 is
	number, number2 : real;
	
	procedure divide(n1: value integer; n2: ref integer) is
	begin
	   n2 := n2 / n1;
	end divide;

begin
	write ("Please Enter two  numbers: ");
	read (number); 
	read (number2);
	divide(number1, number2);
	writeln;
	writeln (int2string(number2), " is the result");
end prog17;

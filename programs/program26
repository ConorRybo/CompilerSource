-- program should result in 4 errors
program prog26 -- should give an error cuz lack of is_sym

begin
	writeln "In ascending order: ";
	for i in 1..5
	lop -- should give an error beacuse of loop_sym typo
		write int2string(i) & " ";
	end loop;
	writeln;
	writeln "In descending order: ; -- should give an error because incomplete string (scanner error cuz incomplete string + parser error because missing semi_colon_sym)
	for i in reverse 1..5
	loop
		write int2string(i) & " ";
	end loop;
	writeln;
end prog26;
